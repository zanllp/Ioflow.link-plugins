{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "typedef":false,
        "object-literal-sort-keys":false,
        "ordered-imports":false,
        "max-line-length": [true, 200],
        "arrow-parens": false,
        "quotemark": [true, "single", "jsx-double"],
        "no-console":false,
        "array-type":false,
        "curly":true,
        "no-shadowed-variable":false,
        "max-classes-per-file":false
    },
    "rulesDirectory": []
}